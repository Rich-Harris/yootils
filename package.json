{
  "name": "yootils",
  "description": "Stuff I often need",
  "version": "0.0.17",
  "type": "module",
  "main": "dist/yootils.cjs",
  "module": "src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/yootils.cjs"
  },
  "types": "types/index.d.ts",
  "files": [
    "src",
    "dist",
    "types"
  ],
  "devDependencies": {
    "agadoo": "^1.0.0",
    "esbuild": "^0.11.12",
    "mocha": "^5.2.0",
    "prettier": "^2.2.1",
    "rollup": "^0.64.1",
    "typescript": "^3.0.1",
    "uvu": "^0.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rich-Harris/yootils.git"
  },
  "scripts": {
    "bundle": "esbuild --bundle src/index.js --format=cjs --outfile=dist/yootils.cjs",
    "types": "typescript src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "build": "npm run types && npm run bundle && agadoo",
    "format": "prettier -w src",
    "test": "uvu src \".\\.spec\\.js\"",
    "prepublishOnly": "npm test && npm run build"
  }
}
